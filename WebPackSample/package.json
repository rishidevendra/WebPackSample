{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress",
    "build": "npm run build",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "server:dev": "npm run build:dev && webpack-dev-server --config config/webpack.dev.js --open --inline --progress --profile --watch",
    "server:prod": "npm run build:prod && webpack-dev-server --config config/webpack.prod.js --open --inline --progress --port 7777 --profile --watch",
    "start": "npm run server:prod",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "lint": "tslint \"app/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@angular/upgrade": "~2.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",    
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6",
    "jquery": "^2.2.4",   
    "angular2-ie9-shims": "^0.0.2",
    "gulp": "^3.9.1",
    "bootstrap": "^3.3.7",    
    "webpack": "2.2.1",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.0.10",
    "tslint": "^4.4.2",
    "gulp": "^3.9.1",
    "codelyzer": "^2.0.0",
    "gulp-tslint": "^7.1.0",
    "@types/node": "^6.0.46",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "angular2-load-children-loader": "^0.1.0",
    "es6-promise-loader": "^1.0.1",
    "bootstrap-loader": "^1.3.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-webpack": "0.0.5",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "node-sass": "^3.11.2",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-merge": "3.0.0",
    "webpack-dev-server": "^1.16.2"
  }
}
